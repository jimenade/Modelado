<?xml version="1.0"?>

<robot name="example2">

    <!-- Base -->
    <link name="ball">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="10.0" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                     iyy="1.0" iyz="0.0" izz="1.0" />

        </inertial>
        <visual>        
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.5" />
            </geometry>
            <material name="blue"/>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
             <sphere radius="0.5" />
            </geometry>
        </collision>
    </link>

    <!-- Material -->
    <material name="blue">
        <color rgba="0 0 0.8 1" />
    </material>


</robot>

